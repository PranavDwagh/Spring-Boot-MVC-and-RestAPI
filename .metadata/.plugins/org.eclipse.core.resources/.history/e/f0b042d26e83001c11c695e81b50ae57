<!DOCTYPE html>
<html lang="en">
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.js"></script>
    <script>
        $(document).ready(()=>{
            $("#b1").click(()=>{
                $.ajax({
                    url:'/grocery/getAll',
                    type:"GET",
                    success:function(data)
                    {
                        var obj = JSON.stringify(data)
                        $("#dis").html(obj);
                    }
                })
            })

            $("#b2").click(()=>{

                $.ajax({
                    url:'/grocery/getAll',
                    type:"GET",
                    success:function(data)
                    {
                        $("#dis").html('');
                        var s = $("<select></select>");
                        for(var i=0; i<data.length;i++)
                        {
                            s.append(data[i].product_name)
                        }
                        $("#dis").append(s);
                    }
                })
            })
        })        
    </script>

</head>
<body>
    <button id="b1">Get All Books</button>
    <button id="b2">Get All Books in DropDown</button>
    <p id="dis"></p>
</body>
</html>